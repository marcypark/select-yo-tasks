{
  "manifest_version": 2,

  "name": "Select Yo Tasks",
  "description": "Add selected text to a to do list!",
  "version": "0.0.1",

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "See yo tasks"
  },

  "permissions": [
    "http://*/*", "https://*/*", "storage"
  ]
}
